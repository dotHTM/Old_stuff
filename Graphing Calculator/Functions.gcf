GraphingCalculator 3.5;Window 51 51 748 574;PaneDivider 588;DrawGraph 0;FontSizes 12 9 6;2D.BottomLeft -0.40625 -0.4375;Text "This file is a collection of functions created to save time.  Copy and paste these into your .gcf files to use.Logical And:";Color 3;Expr function(L_0,a,b)=branch(if(0,a<0),if(0,b<0),1);Text "Logical Or";Color 2;Expr function(L_1,a,b)=function(L_0,a,b)+function(L_0,-a,-b);Text "Three ring Axis device vector points 1->x, 2->y, 3->z, Rings A,B, % C, are controlled by their velocity.  XYZ is the point for the plot to spin";Expr function(R_1,A,B,C,X,Y,Z)=X*[cos(a*n)*cos(b*n)*cos(c*n)-(sin(a*n)*sin(c*n))]-(Y*sin(b*n)*cos(a*n))+Z*[-(sin(c*n)*cos(a*n)*cos(b*n))-(sin(a*n)*cos(c*n))];Color 4;Expr function(R_2,A,B,C,X,Y,Z)=X*sin(b*n)*cos(c*n)+Y*cos(b*n)-(Z*sin(b*n)*sin(c*n));Color 5;Expr function(R_3,A,B,C,X,Y,Z)=X*[sin(a*n)*cos(b*n)*cos(c*n)+sin(c*n)*cos(a*n)]-(Y*sin(a*n)*sin(b*n))+Z*[-(sin(a*n)*sin(c*n)*cos(b*n))+cos(a*n)*cos(c*n)];Text "Change radius over time (f= % drop, q= % time to be used, M= time from 0 to 2 pi)";Color 6;Expr function(F_1,f,q,M)=-(M*f*q)/(2*pi)+1;Text "Square function with T from 0 to 2pi";Color 8;Expr function(B_1,T)=branch(if(4*mod([T,pi/2])/pi-1,0<mod([T,2*pi])<pi/2),if([-4]*mod([T,pi/2])/pi+1,pi<mod([T,2*pi])<3*pi/2),(-mod([T/2,pi])+pi/2)/abs(mod([T/2,pi])-pi/2));Text "Test Bed:";Color 7;XYType 2;Expr function(L_0,x,y);